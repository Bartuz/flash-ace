package {	import flash.display.MovieClip;	public class Main extends MovieClip	{		public function Main()		{			var ileRoabkow = 500;			startGame(500);			function startGame(ilosc_robakow:Number)			{				var kropelki:Array = new Array();				for (var j =0; j <swiat_mc.numChildren; j++)				{					var item = swiat_mc.getChildAt(j);					if (  item is Kropelka )					{						kropelki.push(item);					}				}				for (var i=0; i<ilosc_robakow; i++)				{					var losx = Math.sin(Math.random() * 360);					var losy = Math.sin(Math.random() * 360);					var gdzie_x = Math.floor(Math.random() * 150) * losx;					var gdzie_y = Math.floor(Math.random() * 150) * losy;					var r:Robak = new Robak(gdzie_x,gdzie_y,swiat_mc.biedronka_mc,kropelki);					swiat_mc.addChild(r);				}			}		}	}}